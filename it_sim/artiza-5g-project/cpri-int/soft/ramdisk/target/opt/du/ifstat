			printf("   ULN SEQDUPA0UN    :0x%s 0x%s 0x%s 0x%s\n",Rrh.regread("a2010454"),Rrh.regread("a2010654"),Rrh.regread("a2010854"),Rrh.regread("a2010a54"))
